            var _gachaFoot = instance_create(irandom_range(160,560),i,gacha_foot);
            _gachaFoot.c = _characterID;
        i-=((n9+n8)*n2*n3);}
    }else{room_goto(room_menu);}
}
else{room_goto(room_menu);}
//counter
global.gacha_clicked = n0;
