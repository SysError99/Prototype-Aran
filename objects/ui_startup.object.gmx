            <string>///script
//tap
if(mouse_check_button_released(mb_left)){if(!instance_exists(ui_dialog)){
    if(mouse_y&lt;1120){
        if(mouse_y&gt;=880){state=n1;func_sound_click();}//quit startup screen
    }
    else{
        if(mouse_x&lt;=160){func_sound_click();//en
            global.lang=n0;
            game_translations();
        }
        else if(mouse_x&lt;=300){func_sound_click();//th
            global.lang=n1;
            game_translations();
        }
        else if(mouse_x&lt;=480){func_sound_click();//jp
            global.lang=n2;
            game_translations();
        }
        else{func_sound_click();//credits
            credits0=show_message_async(func_file_read("credits0.txt"));
        }
        //show_dialog_lang_change
        if(mouse_x&lt;=480){
            var d = instance_create(0,0,ui_dialog_alert);
            d.text=global.txt_config;
            d.buttontext=global.txt_ok;
        }
    }
}}
//ui_components
draw_background(bg, 0,0);
draw_background(bg0,0,0);
if(global.lang==n2){//jp overlay
    draw_background(bg1,0,0);
}
draw_background_ext(bgshft,0,0,1,1,0,c_white,abs(cos(degtorad(a))));
//timer
if(a&lt;359){a++;}else{a=n0;}
//anim_in_out
if(state==0){if(stime&lt;59){stime++;}}//anim_in
else {if(stime&gt;0){stime--;}//anim_out
else{
    //clean mem
    background_delete(bg);
    background_delete(bg0);
    background_delete(bg1);
    background_delete(bgshft);
    
    game_translations();//load game translations
    room_goto(room_menu);//load menu
}}
