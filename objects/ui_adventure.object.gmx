            <string>///script
//anim
if(state==0){
    if(stime&lt;60){stime++;}
    //click
    if(mouse_check_button_pressed(mb_left)){if(!(instance_exists(ui_dialog))&amp;&amp;!(instance_exists(ui_team))){
        //bottom_buttons
        if(mouse_y&gt;=1040&amp;&amp;mouse_y&lt;=1160){
            //quit expendition
            if(mouse_x&gt;=40&amp;&amp;mouse_x&lt;=160){func_sound_click();
                //summarise
                func_gameplay_end();
                //close
                state=n1;
            }
            //goto home
            else if(mouse_x&gt;=560&amp;&amp;mouse_x&lt;=680){func_sound_click();
                //goto home
                global.map_file="home";
                state=n1;
            }
        }
    }}
}
else{
    if(stime&gt;0){stime--;}
    else if(global.map_file!=""){
        if(global.map_file == "home"){
            game_save_data();
        }
        room_goto(room_game);
    }
    else{room_goto(room_menu);}
}
var dy = func_dy_reverse(stime,60);
//draw_center
var st=stime/60;
draw_sprite_ext(spr_adventure,0,360,600,2-st,2-st,0,c_white,st);
draw_shadowed_text(360,dy+50,global.txt_adventure,global.font,fa_middle,fa_center,c_black);
//draw_fade
draw_set_colour(c_black);
draw_set_alpha(1-st);
draw_rectangle(0,0,720,1200,false);
draw_set_colour(c_white);
draw_set_alpha(1);
</string>
